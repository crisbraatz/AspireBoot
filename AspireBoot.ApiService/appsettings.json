{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AspireBoot": {
    "AngularHost": "app.localhost",
    "AngularOrigin": "https://app.localhost:1443",
    "AppEnvironment": "production",
    "AppLanguage": "en-US",
    "AspireHost": "api.localhost",
    "Cookie": {
      "Domain": "https://app.localhost:1443",
      "ExpiresAfter": 7
    },
    "Jwt": {
      "Audience": "ASPIRE_BOOT_JWT_AUDIENCE",
      "ExpiresAfter": 60,
      "Issuer": "ASPIRE_BOOT_JWT_ISSUER",
      "SecurityKey": "ASPIRE_BOOT_256_BITS_JWT_SECURITY_KEY"
    },
    "Rabbit": {
      "UserListRequestsCounterConsumer": {
        "Exchange": "user-list-requests-counter-exchange",
        "Queue": "user-list-requests-counter-queue"
      }
    }
  },
  "ConnectionStrings": {
    "AspireBoot": "Host=localhost;Port=5432;Username=aspireboot;Password=postgres;Database=aspireboot;Search Path=public;",
    "Rabbit": "localhost:15672",
    "Redis": "localhost:6379"
  }
}
