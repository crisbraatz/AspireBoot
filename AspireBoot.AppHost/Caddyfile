app.localhost:1443 {
    tls internal

    @spa {
        not path /assets/*
        not path *.css
        not path *.eot
        not path *.gif
        not path *.jpeg
        not path *.jpg
        not path *.js
        not path *.map
        not path *.mp3
        not path *.mp4
        not path *.png
        not path *.svg
        not path *.ttf
        not path *.webm
        not path *.webp
        not path *.wav
        not path *.woff
        not path *.woff2
    }

    rewrite @spa /

    reverse_proxy host.docker.internal:4201
}

https://api.localhost:1443 {
    tls internal
    reverse_proxy host.docker.internal:5101
}

rabbit.localhost:1443 {
    reverse_proxy Rabbit:15672
}

redis.localhost:1443 {
    reverse_proxy RedisCommander:8081
}
